    <script>
      //股票搜索
      function stock_search() {
        $("#search-tip-box").css('display', 'none');
        var params = $('#keyword1').val();
        var url = "http://www.laoqianzhuang.com/index.php?m=content&c=index&a=stock_search", html = '';
        $.ajax({
          type: "get",
          dataType: "jsonp",
          url: url,
          jsonp: "callback",
          jsonpCallback: "stock_callback",
          data: '&stock_code=' + params,
          success: function (data) {
            if (data) {
              var length = data.length;
              if (length > 0) {
                var i = 0;
                var str = '<ul class="search_ul"><li >代码</li><li>名称</li><li>简称</li><li>类型</li></ul>';
                for (i; i < length; i++) {

                  str += "<a href='/quotes/" + data[i]['code'] + "/'>" + '<ul class="stock_ul">';
                  str += '<li>' + data[i]['code'] + '</li>';
                  str += '<li>' + data[i]['name'] + '</li>';
                  str += '<li>' + data[i]['alias'] + '</li>';
                  str += '<li>' + data[i]['type'] + '</li>';
                  str += '</ul></a>';
                  $('#search-tip-box').css('display', 'block');
                  $('.sbox-bd ul').html(str);
                }
              }
            }
          }
        });

      }
      //名家搜索
      function mj_search() {
        $("#search-tip-box").css('display', 'none');
        var params = $('#keyword_mj').val();
        var url = "http://www.laoqianzhuang.com/index.php?m=content&c=index&a=mj_search", html = '';
        $.ajax({
          type: "get",
          dataType: "jsonp",
          url: url,
          jsonp: "callback",
          jsonpCallback: "mj_callback",
          data: '&mj_name=' + params,
          success: function (data) {
            if (data) {
              var length = data.length;
              if (length > 0) {
                var i = 0;
                var str = '';
                for (i; i < length; i++) {
                  str += "<a target='_blank' href='/mj_" + data[i]['userid'] + "/'>" + data[i]['name'] + '&nbsp;&nbsp;&nbsp;&nbsp;' + data[i]['designation'] + '</a><br />';
                  $('#search-tip-box').css('display', 'block');
                  $('.sbox-bd ul').html(str);
                }
              }
            }

          }
        });

      }
      //资讯搜索
      function info_search() {
        $("#search-tip-box").css('display', 'none');
        var params = $('#keyword_zx').val();
        var url = "http://www.laoqianzhuang.com/index.php?m=content&c=index&a=info_search", html = '';
        $.ajax({
          type: "get",
          dataType: "jsonp",
          url: url,
          jsonp: "callback",
          jsonpCallback: "info_callback",
          data: '&title=' + params,
          success: function (data) {
            if (data) {
              var length = data.length;
              if (length > 0) {
                var i = 0;
                var str2 = '';
                for (i; i < length; i++) {
                  str2 += '<a target="_blank" href=' + data[i]['url'] + '>' + data[i]['title'] + '</a><br />';
                  $('#search-tip-box').css('display', 'block');
                  $('.sbox-bd ul').html(str2);
                }

              }
            }
          }
        });

      }

    </script>

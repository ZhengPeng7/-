      get_list(9);
      get_list(25);
      get_list(12);
      function get_list(catid){
          $.ajax({
            type:'get',
            url:'http://www.laoqianzhuang.com/index.php?m=content&c=index&a=getCategoryDatabyCateId&catid='+catid,
            dataType:'jsonp',
            jsonp:"callback",
            scriptCharset: 'utf-8',
            async:false,
            success:function(result){
                var html='';
                data = result.list;
                html += '<div class="ta-parent-box2"><ul class="yahei page_list" tpe-plugin="preview_wap_row">';
                $.each(data,function(i, n){
                    if(i == 0){
                        html += '<li class="first">';
                        html += '<a title="'+ n.title +'" target="_blank" href="'+ n.url +'">';
                        html += '<img src="'+ n.thumb +'" alt="'+ n.title +'" width="121" height="75"> </a>';
                        html += '<a target="_blank" class="title" style="font-weight:bold;" title="'+ n.title +'" href="'+ n.url +'">'+ n.title +'</a></li>';
                    }
                    html += '<li><a href="'+ n.url +'" target="_blank" title="'+ n.title +'">'+ n.title +'</a></li>';
                });
                html += '</ul></div>';
                $(".comment_404").append(html);
            }
        });
      }

  </script>
